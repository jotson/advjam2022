; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="AdvJam2022"
run/main_scene="res://game/checkin/Checkin.tscn"
config/icon="res://icon.png"

[autoload]

Game="*res://singletons/Game.tscn"

[display]

window/size/width=800
window/size/height=450
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/ridiculous_coding/plugin.cfg" )

[importer_defaults]

ogg_vorbis={
"loop": false,
"loop_offset": 0
}

[layer_names]

3d_physics/layer_1="Player"
3d_physics/layer_2="World"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_clear_color=Color( 0.164706, 0.533333, 0.886275, 1 )
environment/default_environment="res://default_env.tres"
